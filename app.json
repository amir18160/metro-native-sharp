{
    "expo": {
        "name": "Metro Moviez",
        "slug": "metro-moviez",
        "version": "1.0.0",
        "scheme": "Metro Moviez",
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            "expo-router",
            "expo-web-browser",
            [
                "expo-build-properties",
                {
                    "android": {
                        "usesCleartextTraffic": true
                    },
                    "ios": {
                        "deploymentTarget": "15.1"
                    }
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true,
            "tsconfigPaths": true
        },
        "orientation": "portrait",
        "icon": "./assets/logo.png",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/splash.jpg",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            }
        }
    }
}
